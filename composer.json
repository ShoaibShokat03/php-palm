{
  "name": "php-palm/project",
  "description": "PHP Palm starter project",
  "type": "project",
  "version": "0.1.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Muhammad Shoaib",
      "email": "shoaibshokat6@gmail.com"
    }
  ],
  "require": {
    "vlucas/phpdotenv": "^5.6"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "App\\Modules\\": "modules/",
      "Config\\": "config/",
      "PhpPalm\\Core\\": "app/core/",
      "Frontend\\": "src/",
      "Frontend\\Palm\\": "app/Palm/"
    },
    "files": [
      "app/Palm/helpers.php"
    ]
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php -r \"copy('.env.example', '.env');\""
    ]
  }
}
